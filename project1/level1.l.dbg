%{
#include <stdio.h>
#include "y.tab.h"
%}

%%

-?[0-9]+                                yylval=atoi(yytext); return INT;
-?([0-9]+\.[0-9]*)|([0-9]*\.[0-9]+)     yylval=atof(yytext); return FLOAT;
[_a-zA-Z][a-zA-Z0-9_]*                  yylval=yytext; return IDENT;
\[                                      return LBRACKET;
\]                                      return RBRACKET;
\(                                      return LPAREN;
\)                                      return RPAREN;
%%
